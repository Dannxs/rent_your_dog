<div class="row mt-5">
  <div class="col-12 col-lg-6 offset-lg-3">
    <h1>Start your adventure with <%= @dog.name %> the dog</h1>
    <%= simple_form_for [ @dog, @booking ] do |f| %>
      <%= f.input :start_date, as: :string, required: false, input_html: {id: "range_start"} %>
      <%= f.input :end_date, as: :string, required: false, input_html: {id: "range_end"} %>
      <%= f.submit "Submit booking", class: "btn btn-success" %>
    <% end %>
  </div>
</div>
